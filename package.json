{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "atlassian-design-tokens",
  "title": "Atlassian Design Tokens",
  "description": "Lists Atlassian design tokens",
  "icon": "extension_icon.png",
  "author": "acampbell3",
  "categories": [
    "Design Tools",
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Design Tokens",
      "subtitle": "View all Atlassian design tokens",
      "description": "Shows a list of all design tokens, along with details on usage and other metadata",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@atlaskit/tokens": "^1.28.1",
    "@raycast/api": "^1.62.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
